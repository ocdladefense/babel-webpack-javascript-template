{
  "name": "myapp",
  "version": "0.1.0",
  "description": "My app.",
  "main": "view.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel-build": "babel src --out-dir dist --source-maps",
    "build": "webpack --config webpack.config.js --env USE_MOCK --env MODULE_PATH=@local/lod",
    "update": "cp package.json package-main.json && package-json-merge package-main.json package-overrides.json > package.json",
    "save": "cp package.json package-orig.json",
    "reset": "cp package-orig.json package.json",
    "start": "webpack serve",
    "lint": "eslint src/**",
    "lint-fix": "eslint --fix src/**",
    "git-reset": "git reset --soft HEAD~1",
    "git-force": "git push origin +main --force",
    "watch": "webpack serve --open --env USE_MOCK --env MODULE_PATH=@local/lod",
    "build:ocdla:publish": "webpack --config webpack.config.js --env USE_MOCK --env MODULE_PATH=@local/publish",
    "build:ocdla:ors": "webpack --config webpack.config.js --env USE_MOCK --env MODULE_PATH=@local/references",
    "build:ocdla:openai": "webpack --config webpack.config.js --env USE_MOCK --env MODULE_PATH=@local/openai",
    "watch:ocdla:publish": "webpack serve --open --env USE_MOCK --env MODULE_PATH=@local/publish",
    "watch:ocdla:ors": "webpack serve --open --env USE_MOCK --env MODULE_PATH=@local/references",
    "watch:ocdla:openai": "webpack serve --open --env USE_MOCK --env MODULE_PATH=@local/openai"
  },
  "author": "Jos√© Bernal",
  "license": "ISC",
  "dependencies": {
    "@local/openai": "file:./dev_modules/@local/openai",
    "@local/lod": "file:./dev_modules/@local/lod",
    "@local/publish": "file:./dev_modules/@local/publish",
    "@local/references": "file:./dev_modules/@local/references",
    "@ocdla/global-components": "file:./dev_modules/@ocdla/global-components",
    "@ocdla/lib-http": "file:./dev_modules/@ocdla/lib-http",
    "@ocdla/view": "file:./dev_modules/@ocdla/view",
    "@ocdladefense/ors": "file:./dev_modules/@ocdla/ors",
    "@ocdla/lib-routing": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "dotenv": "^16.4.5",
    "globals": "^15.6.0",
    "openai": "^4.67.3",
    "postcss": "^8.4.39",
    "tailwindcss": "^3.4.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.24.7",
    "@tailwindcss/forms": "^0.5.9",
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.11.0",
    "dotenv-webpack": "^8.1.0",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.5.4",
    "jsdoc": "^4.0.2",
    "package-json-merge": "git+https://github.com/ocdladefense/package-json-merge.git",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
